{
  "extends": "./tsconfig.expo.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@theme/*": [
        "theme/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@app-types/*": [
        "types/*"
      ],
      "@context/*": [
        "context/*"
      ]
    },
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
